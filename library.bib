journal = {Journal of Building Engineering},
keywords = {Asset management,BIM standards,Building information modelling,Facilities management,Mechanical and electrical engineering},
number = {May},
pages = {101540},
publisher = {Elsevier Ltd},
title = {{Improving BIM asset and facilities management processes: A Mechanical and Electrical (M{\&}E) contractor perspective}},
