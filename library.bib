doi = {10.1016/j.proeng.2014.10.525},
file = {:C$\backslash$:/Users/dekao/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Ant{\'{o}}n, D{\'{i}}az - 2014 - Integration of life cycle assessment in a BIM environment.pdf:pdf},
