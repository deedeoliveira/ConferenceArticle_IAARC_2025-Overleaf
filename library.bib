issn = {09265805},
journal = {Automation in Construction},
keywords = {Activity theory,Building information model (BIM),Operations {\&} Maintenance (O{\&}M),Status analysis},
number = {November 2017},
pages = {317--332},
title = {{Activity theory-based analysis of BIM implementation in building O{\&}M and first response}},
