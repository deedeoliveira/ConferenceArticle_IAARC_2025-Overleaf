doi = {10.1016/j.autcon.2019.102940},
file = {:C$\backslash$:/Users/dekao/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/El Ammari, Hammad - 2019 - Remote interactive collaboration in facilities management using BIM-based mixed reality.pdf:pdf},
issn = {09265805},
journal = {Automation in Construction},
keywords = {Building information modeling,Facilities management,Feature-based tracking,Mixed reality,Remote interactive visual collaboration},
number = {January},
pages = {102940},
publisher = {Elsevier},
title = {{Remote interactive collaboration in facilities management using BIM-based mixed reality}},
