doi = {10.1080/17452007.2019.1706439},
file = {:C$\backslash$:/Users/dekao/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Munir et al. - 2020 - BIM-based operational information requirements for asset owners.pdf:pdf},
