@article{Tay2001,
   abstract = {Despite its rapid development in the last decade, facilities management (FM) stills suffers from an identity crisis as the definition and scope of FM remains a contentious issue. To this end, three fundamental issues are re-examined in this paper: what FM constitutes; what a facility manager is; and how the FM profession can be enhanced. These issues remain critical as they represent the building blocks of the FM discipline. Without a common platform, the development of FM is likely to be fragmented. An evaluation of the definitions of FM provided in the past suggests that the focus of FM is clearly on the workplace. The key issues confronting FM are the location, type, quantity, quality, content and allocation of the workspace. A professional facilities manager is one who is formally trained and whose main responsibility is the strategic management of the workplace. Three factors are suggested to be important for the development of FM as a professional discipline. They include a clear role and scope of FM in the industry and firm, contribution to the bottom-line of the firm, and development of specialist knowledge and toolbox for addressing the problems of strategic workplace management. Some potential areas for theoretical developments have been suggested in this paper. © 2001, MCB UP Limited},
   author = {Linda Tay and Joseph T.l. Ooi},
   doi = {10.1108/EUM0000000005534},
   issn = {02632772},
   issue = {10},
   journal = {Facilities},
   keywords = {Facilities management,Improvement,Professionalism,Strategic management},
   pages = {357-363},
   title = {Facilities management: A “Jack of all trades”?},
   volume = {19},
   year = {2001},
}
@article{Yalcinkaya2014,
   abstract = {La gestión de instalaciones (FM) es una disciplina que comprende diversas operaciones, actividades y servicios de mantenimiento para apoyar las principales funciones de un edificio o instalación en uso. Exige conjuntos completos de información sobre la instalación. Si bien actualmente se utilizan varios sistemas de información fm para gestionar dicha información, la información gráfica y no gráfica multifacísica almacenada en los modelos de información de construcción (BIM) de la fase de preutilidad no se ha integrado suficientemente en los sistemas FM existentes. BIM, con sus capacidades de visualización, interoperabilidad e intercambio de información, puede agilizar las actividades de FM. Por lo tanto, el uso de BIM para FM ha ganado interés mundial en la investigación. Este artículo analiza esta tendencia basándose en el análisis cualitativo de la literatura de última generación sobre el tema. En particular, este documento examina la naturaleza de BIM y FM dentro de su variedad de funciones e interacciones. El análisis se centra en el potencial de valor agregado de BIM e informa de los hallazgos para designar los beneficios de BIM para FM, y evaluar los posibles desafíos que están obstaculizando el uso efectivo de BIM en FM.},
   author = {Mehmet Yalcinkaya and Vishal Singh},
   journal = {Ifip Aict},
   keywords = {am,asset management,bim,building information modeling,facilities management,fm},
   pages = {1-10},
   title = {Building Information Modeling ( BIM ) for Facilities Management – Literature},
   volume = {442},
   year = {2014},
}
