   keywords = {Industry 4.0,Internet of Things,Ontology,Semantic web technologies},
   pages = {507-515},
   publisher = {Springer Nature Singapore},
   title = {A real-time semantic based approach for modeling and reasoning in Industry 4.0},
   volume = {16},
