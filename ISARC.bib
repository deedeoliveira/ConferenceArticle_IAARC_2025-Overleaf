@article{Messner2011,
   abstract = {A Building Information Model (BIM) is “a digital representation of physical and functional characteristics of a facility.”1 To successfully implement BIM, a project team must perform detailed and comprehensive planning. A well-documented BIM Project Execution Plan will ensure that all parties are clearly aware of the opportunities and responsibilities associated with the incorporation of BIM into the project workflow. A completed BIM Project Execution Plan should define the appropriate Uses for BIM on a project (e.g., design authoring, design review, and 3D coordination), along with a detailed design and documentation of the process for executing BIM throughout a facility’s lifecycle. Once the plan is created, the team can follow and monitor their progress against this plan to gain the maximum benefits from BIM implementation.},
   author = {John Messner and Chimay Anumba and Craig Dubler and Shane Goodman and Colleen Kasprzak and Ralph Kreider and Robert Leicht and Chitwan Saluja and Nevena Zikic},
